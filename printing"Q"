#include<stdio.h>
int main()
{
    int rows=10,i,j,d=rows;
    for (i = 0; i<rows/2; i++) 
    {
        printf("*");
        for (j = 0; j <=rows; j++) 
        {
            if ((i==rows || i == rows-1) && j <rows)
                printf("*");
            else if (j == rows-1 && i != 0 && i !=rows)

                printf("*");
            else
                printf(" ");
        }
        printf("\n");
    
        d++;
    }

}
