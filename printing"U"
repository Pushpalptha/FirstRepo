#include<stdio.h>
int main()
 {  
    int rows=10,i, j;
    for (i = 0; i <rows; i++)
    {
        if (i != 0 && i != rows - 1)
            printf("*");
        else
            printf(" ");
        for (j = 0; j <rows; j++) 
        {
            if (((i ==rows- 1)  && j >= 0 && j <rows- 1))
                printf("*");
            else if (j == rows- 1 && i != 0 && i !=rows- 1)
                printf("*");
            else
                printf(" ");
        }
        printf("\n");
    }
}
