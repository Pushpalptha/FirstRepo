include<stdio.h>
int main()
{
    int rows=10,i,j;
    
    
        for(i=2;i<=rows;i++)
    {
        for(j=1;j<=rows;j++)
        {
            if(j==i || j == rows-i)
               printf("*");
            else
              printf(" ");
        }
        printf("\n");
    }
         
    return 0;
}
