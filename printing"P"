#include<stdio.h>

int main()
{
  int rows=10,i, j;
    for (i = 0; i <rows; i++) {
        printf("*");
        for (j = 0; j <rows; j++) {
            if ((i == 0 || i == rows / 2)
                && j < rows - 1)
                printf("*");
            else if (i < rows/ 2
                    && j ==rows- 1 && i != 0)
                printf("*");
            else
                printf(" ");
        }
        printf("\n");
    }

}
