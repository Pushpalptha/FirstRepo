#include<stdio.h>
int main()
{
    int i, j,rows=10,n=0;
    for (i = 0; i <rows; i++) 
    {
        printf("*");
        for (j = 0; j <=rows; j++) 
        {
            if (j == rows)
                printf("*");
            else if (j == n)
                printf("*");
            else
                printf(" ");
        }
        n++;
        printf("\n");
    }
}
