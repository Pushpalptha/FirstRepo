#include<stdio.h>
int main()
{
    int i, j,rows=10,k=(2*rows)-1,n= 0;
    for (i = 0; i <rows; i++) 
    {
        for (j = 0; j <=k; j++) 
        {
            if (j ==n || j == k-n- 1)
                printf("*");
            else
                printf(" ");
        }
        n++;
        printf("\n");
    }

}
