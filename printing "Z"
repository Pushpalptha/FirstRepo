#include<stdio.h>
int main()
{
    int rows=10,i,j;
    for(i=1;i<=10;i++)
       printf("*");
    printf("\n");
    for(i=2;i<=rows-1;i++)
    {
        for(j=1;j<=rows;j++)
        {
            if(j == rows-i)
               printf("*");
            else
              printf(" ");
        }
        printf("\n");
    }
         
    for(i=1;i<=rows;i++)
        printf("*");
    return 0;
}
