
#include<stdio.h> 
int main()
{   
    int rows=10,i, j;
    for (i = 0; i <rows; i++) 
    {
        printf("*");
        for (j = 0; j <rows; j++) 
        {
            if ((j == rows- 1) || (i == rows/ 2))
                printf("*");
            else
                printf(" ");
        }
        printf("\n");
    }
}
