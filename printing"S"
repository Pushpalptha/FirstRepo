#include<stdio.h>
int main()
{
     
     int rows=10, i, j;
    for (i = 0; i <rows; i++) {
        for (j = 0; j <rows; j++) {
            if ((i == 0 || i == rows/ 2 || i == rows- 1))
                printf("*");
            else if (i < rows/ 2 && j == 0)
                printf("*");
            else if (i > rows/ 2 && j ==rows- 1)
                printf("*");
            else
                printf(" ");
        }
        printf("\n");
    }
}
